{
  "extends": "tslint:recommended",
  "rules": {
    "no-console": false,
    "no-empty": false,
    "no-unused-variable": [
      true,
      { "ignore-pattern": "^_" }
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "quotemark": [ true, "single" ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ]
  }
}
