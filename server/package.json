{
  "name": "react-watchlist-server",
  "version": "0.0.1",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack",
    "build:watch": "webpack --watch",
    "build:production": "cross-env NODE_ENV=production npm run build",
    "build:tsc": "tsc",
    "build:tsc:w": "tsc -w",
    "pretest": "npm run build",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm test -- --watch",
    "test:notify": "npm run test:watch -- --notify",
    "coverage": "npm test -- --coverage",
    "coverage:notify": "npm run coverage -- --watch --notify",
    "start": "npm run build && node dist/main.js",
    "start:watch": "concurrently \"npm run build:watch\" \"node-dev dist/main.js\"",
    "preupver": "npm test",
    "upver": "standard-version"
  },
  "keywords": [
    "backend",
    "typescript",
    "webpack",
    "express"
  ],
  "author": "Marcin Zolkiewski",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.0",
    "cors": "^2.8.1",
    "cross-env": "^3.2.4",
    "cuid": "^1.3.8",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.2",
    "helmet": "^3.10.0",
    "inversify": "^4.10.0",
    "inversify-express-utils": "^5.2.1",
    "json-loader": "^0.5.4",
    "jwt-simple": "^0.5.1",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "mysql": "^2.15.0",
    "prettyjson": "^1.2.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "slug": "^0.9.1",
    "source-map-loader": "^0.1.6",
    "typeorm": "^0.1.12",
    "unique-slug": "^2.0.0",
    "webpack-merge": "^4.1.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.1",
    "@types/body-parser": "0.0.33",
    "@types/chalk": "^0.4.31",
    "@types/cors": "2.8.0",
    "@types/dotenv": "^2.0.23",
    "@types/errorhandler": "0.0.30",
    "@types/es6-shim": "^0.31.35",
    "@types/express": "^4.11.1",
    "@types/helmet": "0.0.33",
    "@types/jest": "^20.0.8",
    "@types/jwt-simple": "^0.5.33",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.9.0",
    "@types/sequelize": "^4.27.5",
    "@types/slug": "^0.9.0",
    "awesome-typescript-loader": "^3.4.1",
    "concurrently": "^3.5.1",
    "jest": "^22.2.1",
    "jest-cli": "^22.2.1",
    "node-dev": "^3.1.3",
    "standard-version": "^4.3.0",
    "ts-jest": "^22.0.3",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.7.1",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "mapCoverage": true,
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
