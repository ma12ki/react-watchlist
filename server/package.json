{
  "name": "ng-watchlist-server",
  "version": "0.0.1",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack",
    "build:watch": "webpack --watch",
    "build:production": "cross-env NODE_ENV=production npm run build",
    "build:tsc": "tsc",
    "build:tsc:w": "tsc -w",
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:notify": "npm run test:watch -- --notify",
    "coverage": "npm test -- --coverage",
    "coverage:notify": "npm run coverage -- --watch --notify",
    "start": "npm run build && node dist/main.js",
    "start:watch": "concurrently \"npm run build:watch\" \"node-dev dist/main.js\"",
    "preupver": "npm test",
    "upver": "standard-version"
  },
  "keywords": [
    "apollo",
    "backend",
    "apollo-server",
    "typescript",
    "webpack",
    "graphql",
    "graphiql",
    "express"
  ],
  "author": "Marcin Zolkiewski",
  "license": "MIT",
  "dependencies": {
    "@types/bcryptjs": "^2.4.0",
    "@types/body-parser": "0.0.33",
    "@types/cors": "2.8.0",
    "@types/dotenv": "^2.0.20",
    "@types/express": "^4.0.35",
    "@types/helmet": "0.0.33",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/mongoose": "^4.7.7",
    "@types/morgan": "^1.7.32",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.16.0",
    "cors": "^2.8.1",
    "cross-env": "^3.2.4",
    "cuid": "^1.3.8",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "graphql": "0.9.1",
    "graphql-server-express": "^0.6.0",
    "graphql-subscriptions": "^0.3.0",
    "graphql-tools": "0.10.0",
    "helmet": "^3.5.0",
    "inversify": "^4.2.0",
    "inversify-express-utils": "^4.0.0",
    "json-loader": "^0.5.4",
    "jsonwebtoken": "^7.3.0",
    "moment": "^2.17.1",
    "mongoose": "^4.8.5",
    "morgan": "^1.7.0",
    "mysql2": "^1.3.6",
    "rimraf": "^2.6.1",
    "sequelize": "^4.4.2",
    "source-map-loader": "^0.1.6",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "@types/es6-shim": "^0.31.32",
    "@types/graphql": "^0.8.6",
    "@types/jest": "^18.0.0",
    "@types/node": "^7.0.4",
    "@types/sequelize": "^4.0.66",
    "awesome-typescript-loader": "^3.0.3",
    "concurrently": "^3.1.0",
    "jest": "^18.1.0",
    "jest-cli": "^18.1.0",
    "node-dev": "^3.1.3",
    "standard-version": "^4.0.0",
    "ts-jest": "^18.0.1",
    "tslint": "^5.1.0",
    "tslint-loader": "^3.5.2",
    "typescript": "^2.2.2",
    "webpack": "^2.3.2",
    "webpack-node-externals": "^1.5.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
